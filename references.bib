@unpublished{venkataraman2018,
  title = {Data fitting using {{Natural Bernstein-B{\'e}zier}} functions},
  author = {Venkataraman, Panchapakesan},
  year = 2018,
  abstract = {Data generation, collection, and analysis is occurring at an unimaginable scale today. Machine learning and data analytics are new fields that focus on modelling for business decision making and prediction. In engineering and science disciplines data handling is dominated by functional analysis delivering data smoothing, fitting, modelling, and prediction. In this pursuit the data is transformed into mathematical representations that is more portable and lends easily to additional processing. They can also serve as a surrogate stand-in in for more complex analysis. While B-splines are the basic tool for functional data analysis, in many instances B{\'e}zier functions have represented and smoothed the original data with excellent fidelity. In this paper we introduce the natural B{\'e}zier functions and demonstrate that it delivers similar performance in fitting data and extracting additional information. The natural B{\'e}zier function employs a simple constraint on the vertices of the regular B{\'e}zier function and in the process transfers the properties of the fitted function from the parameter space to the function space. The natural B{\'e}zier functions are eminently portable and can be represented by a simple numerical arrays. Three examples are used to illustrate the smoothing and fitting properties of these functions based on sparse data.},
  langid = {english}
}

