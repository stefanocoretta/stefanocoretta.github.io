@article{sui2022,
  title = {What to watch: {{Practical}} considerations and strategies for using {{YouTube}} for research},
  shorttitle = {What to watch},
  author = {Sui, Wuyou and Sui, Anna and Rhodes, Ryan E},
  year = 2022,
  month = jan,
  journal = {DIGITAL HEALTH},
  volume = {8},
  pages = {205520762211237},
  issn = {2055-2076, 2055-2076},
  doi = {10.1177/20552076221123707},
  urldate = {2025-04-18},
  abstract = {YouTube is the second-most visited webpage in the world and boasts over 2 billion users and 500 h of videos uploaded every hour. Despite this popularity, relatively few articles have discussed the practical use of searching and YouTube as a research tool and source of data. The purpose of our paper is to propose a step-by-step schematic for utilizing the YouTube platform. Our discussions include (a) when/whether to use YouTube for research; (b) selecting an appropriate research design; (c) how to search for YouTube data; (d) what data can be pulled from YouTube; and (e) the contextual limitations for interpreting YouTube data. Further, we provide practical strategies and considerations when searching, collecting, or interpreting YouTube data. These discussions are informed by our own work using the YouTube platform. Effective methods used to search for YouTube data are likely to extend beyond simply searching the platform itself; the search strategy and search results themselves should also be documented. While not exhaustive, we feel these considerations and strategies present themselves as a conceptual foothold for future research using the YouTube platform.},
  langid = {english}
}

@misc{sulik2023,
  title = {Why {{Do Scientists Disagree}}? {{A}} study of the psychological sciences},
  shorttitle = {Why {{Do Scientists Disagree}}?},
  author = {Sulik, Justin and Rim, Nakwon and Pontikes, Elizabeth and Evans, James and Lupyan, Gary},
  year = 2023,
  month = jul,
  publisher = {PsyArXiv},
  doi = {10.31234/osf.io/9bd8k},
  urldate = {2025-04-15},
  abstract = {Scientific research is often characterized by schools of thought. We investigate whether these divisions arise in part from differences in researchers' cognitive traits such as their tolerance for ambiguity and the vividness of their visual imagery. These differences may guide researchers to prefer different types of problems, to tackle the same problems in different ways, and perhaps even reach different conclusions when studying the same problems in the same way. We surveyed 7973 researchers in psychology and allied disciplines and investigated links between what they research (area, topic, methodology), their stances on open questions in psychological science, and a set of validated scales assessing a variety of cognitive traits and dispositions. We also linked responses to publication histories from which we constructed landscapes of similarity and difference based on publication titles and abstracts, citations and co-authorship patterns. Our results show that researchers' stances on scientific questions are, as expected, predictable from what kind of research they do. But they are also predictable from their cognitive traits, even when controlling for their research areas, methods and topics and co-authorship and citation patterns. These findings support the idea that divisions in scientific fields reflect not only differences in the kinds of data brought to bear on a topic, but also differences in the researchers themselves, hinting that the divisions may be more difficult to bridge than suggested by the traditional model of data-driven scientific consensus.},
  copyright = {https://creativecommons.org/licenses/by/4.0/legalcode}
}

